<!DOCTYPE html>
<html lang="en">
<head>
    {{!-- META --}}
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta property="og:title" content="{{teamNickName}} on Scoultimate">
    <meta property="og:description" content="View team information for {{teamNickName}} on Scoultimate">
    <meta property="og:image" content="http://res.cloudinary.com/dk6fgyqvh/image/upload/v1594072427/{{teamNumber}}">

    {{!-- Material Components --}}
    
    <link rel="stylesheet" href="https://unpkg.com/material-components-web@latest/dist/material-components-web.min.css">
    <script defer src="https://unpkg.com/material-components-web@latest/dist/material-components-web.min.js"></script>

    {{!-- General CSS Imports --}}

    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="../css/common.css">

    <title>{{teamData.nickname}} | Scoultimate</title>

    {{!-- General JS Imports --}}

    <script defer src="https://www.googleoptimize.com/optimize.js?id=OPT-WPS9HFR"></script>
    <script defer src="../js/materiallib.js"></script>

    {{!-- FONTS --}}

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Rubik:300,400,500">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto+Mono">

</head>
<body data-barba="wrapper">
    <ul class="transition">
  <li></li>
  <li></li>
  <li></li>
  <li></li>
  <li></li>
</ul>
   <aside class="mdc-drawer mdc-drawer--modal">
    <div class="mdc-drawer__header">
      <h3 class="mdc-drawer__title" src="signin.html">Dashboard</h3>
      <h6 class="mdc-drawer__subtitle trigger">{{#if userData.email}} {{userData.email}} {{else}} <a href="javascript:void(0)"  class="link signin dashboard-signin" id="dashboard-signin">Sign In</a>{{/if}}</h6>
    </div>
    <div class="mdc-drawer__content">
      <nav class="mdc-list">
        <a class="mdc-list-item mdc-list-item--activated dashboard-account" href="#" tabindex="0" aria-current="page">
          <i class="material-icons mdc-list-item__graphic avatar avatar2" aria-hidden="true" src="signin.html">{{#if userData.picture}}<img class="pfp" src={{userData.picture}}> {{else}} account_circle{{/if}}</i>
          <span class="mdc-list-item__text dashboard-account" id="dashboard-account" href="signin.html">My Account</span>
        </a>
        <a class="mdc-list-item" href="#" tabindex="0">
          <i class="material-icons mdc-list-item__graphic" aria-hidden="true">all_out</i>
          <span class="mdc-list-item__text">My Team</span>
        </a>
        <a class="mdc-list-item" href="#" tabindex="0">
          <i class="material-icons mdc-list-item__graphic" aria-hidden="true">search</i>
          <span class="mdc-list-item__text">Search</span>
        </a>
        <a class="mdc-list-item" href="#" tabindex="0">
          <i class="material-icons mdc-list-item__graphic" aria-hidden="true">notes</i>
          <span class="mdc-list-item__text">Scouting</span>
        </a>
        <a class="mdc-list-item" href="#" tabindex="0">
          <i class="material-icons mdc-list-item__graphic" aria-hidden="true">miscellaneous_services</i>
          <span class="mdc-list-item__text">Other Services</span>
        </a>
        <a class="mdc-list-item" href="#" tabindex="0">
          <i class="material-icons mdc-list-item__graphic" aria-hidden="true">info</i>
          <span class="mdc-list-item__text">Info</span>
        </a>
      </nav>
    </div>
  </aside>

  <div class="mdc-drawer-scrim"></div>

  <div class="mdc-drawer-app-content">

    <header class="mdc-top-app-bar mdc-top-app-bar--fixed">
      <div class="mdc-top-app-bar__row">
        <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start" id="headerC">
          <button class="material-icons mdc-top-app-bar__navigation-icon mdc-icon-button">menu</button>
          <span class="mdc-top-app-bar__title"></span>

          

        </section>
      </div>
    </header>

    <main class="mdc-top-app-bar--fixed-adjust">

      
  </div>
    <main data-barba="container" data-barba-namespace="team">
      {{!-- PAGE SPECIFIC CSS COMPONENTS --}}
      
      <link rel="stylesheet" href="../css/team.css">
      <script defer src="https://unpkg.com/grade-js/docs/dist/grade.js"></script>
            {{!-- <div class="bcgrd"> --}}
              <div class="team-profile--grid">
             <div class="team-profile--flex-team-data-img-name">
  <div class="team-logo-wrap"></div><div class="gradient-wrap" ><img class="team-logo-img" src="data:image/png;base64, {{teamData.picture}}"/></div></div>
            <h2 class="team-nickname">{{teamData.nickname}}</h2>
                </div>
        <h3 style="color: white;">{{teamData.awards.length}} Awards</h3>
        <h3 style="color: white;">{{teamData.years.length}} Years</h3>

        <h5>{{teamData.name}}</h5>
        
        {{!-- <h3 style="color: white;">{{teamData.years}}</h3> --}}
        {{!-- <a href="/">2264</a> --}}

      {{!-- </div> --}}
      <div class="mdc-tab-bar" role="tablist">
<div class="mdc-tab-bar" role="tablist">
  <div class="mdc-tab-scroller">
    <div class="mdc-tab-scroller__scroll-area">
      <div class="mdc-tab-scroller__scroll-content">
        <button class="mdc-tab mdc-tab--active" role="tab" aria-selected="true" tabindex="0">
          <span class="mdc-tab__content">
            <span class="mdc-tab__text-label">Events</span>
          </span>
          <span class="mdc-tab-indicator mdc-tab-indicator--active">
            <span class="mdc-tab-indicator__content mdc-tab-indicator__content--underline"></span>
          </span>
          <span class="mdc-tab__ripple"></span>
        </button>
        <button class="mdc-tab mdc-tab" role="tab" aria-selected="true" tabindex="0">
          <span class="mdc-tab__content">
            <span class="mdc-tab__text-label">Awards</span>
          </span>
          <span class="mdc-tab-indicator mdc-tab-indicator">
            <span class="mdc-tab-indicator__content mdc-tab-indicator__content--underline"></span>
          </span>
          <span class="mdc-tab__ripple"></span>
        </button>
        <button class="mdc-tab mdc-tab" role="tab" aria-selected="true" tabindex="0">
          <span class="mdc-tab__content">
            <span class="mdc-tab__text-label">Other</span>
          </span>
          <span class="mdc-tab-indicator mdc-tab-indicator">
            <span class="mdc-tab-indicator__content mdc-tab-indicator__content--underline"></span>
          </span>
          <span class="mdc-tab__ripple"></span>
        </button>
      </div>
    </div>
  </div>
</div>
<div class="content content--active">

  {{!-- <div class="mdc-card mdc-card--outlined">
  <!-- ... content ... -->
</div> --}}
{{!-- {{#if events.length >= 1}} --}}
{{#each teamData.events}}
<div class="mdc-card">
  <div class="mdc-card__primary-action" tabindex="0">
    {{!-- <div class="mdc-card__media mdc-card__media--16-9 mdc-card__media" style="background-image: url(&quot;https://material-components.github.io/material-components-web-catalog/static/media/photos/3x2/2.jpg&quot;);"></div> --}}
    <div class="mdc-card__primary">
      <h2 class="mdc-card__title mdc-typography mdc-typography--headline6 event-name"><a class="" href="/event/{{this.key}}" target="_blank"></a>{{this.name}}</h2>
      <h3 class="mdc-card__subtitle mdc-typography mdc-typography--subtitle2"><a class="event-location--link" href={{this.gmaps_url}}>{{this.location_name}} | Week {{this.week}} - {{this.start_date}} to {{this.end_date}}</a></h3>
    </div>
    <div class="mdc-card__secondary mdc-typography mdc-typography--body2">Visit ten places on our planet that are undergoing the biggest changes today.</div>
  </div>
  <div class="mdc-card__actions">
    <div class="mdc-card__action-buttons">
      <a href="/event/{{this.key}}"><button class="mdc-button mdc-card__action mdc-card__action--button" >  <span class="mdc-button__ripple"></span> View Event</button></a>
      {{!-- <button class="mdc-button mdc-card__action mdc-card__action--button">  <span class="mdc-button__ripple"></span> Bookmark</button> --}}
    </div>
    <div class="mdc-card__action-icons">
      <button class="mdc-icon-button mdc-card__action mdc-card__action--icon--unbounded" aria-pressed="false" aria-label="Add to favorites" title="Add to favorites">
        <i class="material-icons mdc-icon-button__icon mdc-icon-button__icon--on">favorite</i>
        <i class="material-icons mdc-icon-button__icon">favorite_border</i>
      </button>
      <button class="mdc-icon-button material-icons mdc-card__action mdc-card__action--icon--unbounded" title="Share" data-mdc-ripple-is-unbounded="true">share</button>
      <button class="mdc-icon-button material-icons mdc-card__action mdc-card__action--icon--unbounded" title="More options" data-mdc-ripple-is-unbounded="true">more_vert</button>
    </div>
  </div>
</div>
{{/each}}

</div>
<div class="content">
  <p>Content two</p>
</div>
<div class="content">
  <p>Content three</p>
</div>

</div>

      <style>.content {
  display: none;
}

.content--active {
  display: block;
}</style>
<script>    window.addEventListener('load', function(){
        /*
            A NodeList of all your image containers (Or a single Node).
            The library will locate an <img /> within each
            container to create the gradient from.
         */
        Grade(document.querySelectorAll('.gradient-wrap'))
        const tabScroller = new mdc.tabScroller.MDCTabScroller(document.querySelector('.mdc-tab-scroller'));
    })</script>
</main>


  {{!-- NEED THESE IMPORTS EVERY TIME (ALONGSIDE MDC) --}}
  <script defer src="https://cdn.jsdelivr.net/npm/@barba/core"></script>
  <script defer src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.2.4/gsap.min.js"></script>
  <script defer src="../js/transitions.js"></script>
  <script defer src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

</body>
</html>